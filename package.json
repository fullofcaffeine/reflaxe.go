{
  "name": "reflaxe.go",
  "private": true,
  "version": "0.1.0",
  "license": "GPL-3.0-only",
  "description": "Reflaxe-based Go target for Haxe",
  "scripts": {
    "postinstall": "lix download",
    "dev:new-project": "bash scripts/dev/new-project.sh",
    "dev:hx": "bash scripts/dev/go-hx.sh",
    "test": "python3 test/run-snapshots.py",
    "test:ci": "python3 test/run-ci.py",
    "test:examples": "python3 test/run-examples.py",
    "test:examples:changed": "python3 test/run-examples.py --changed",
    "test:list": "python3 test/run-snapshots.py --list",
    "test:negative": "python3 test/run-snapshots.py --category negative",
    "test:go-native": "python3 test/run-snapshots.py --category go_native",
    "test:failed": "python3 test/run-snapshots.py --failed",
    "test:changed": "python3 test/run-snapshots.py --changed",
    "test:update": "python3 test/run-snapshots.py --update",
    "test:bless": "python3 test/run-snapshots.py --bless",
    "test:semantic-diff": "python3 test/run-semantic-diff.py",
    "test:stdlib-sweep": "python3 test/run-upstream-stdlib-sweep.py",
    "test:stdlib-sweep:strict": "python3 test/run-upstream-stdlib-sweep.py --strict",
    "test:stdlib-sweep:go-test": "python3 test/run-upstream-stdlib-sweep.py --strict --go-test",
    "test:stdlib-sweep:full": "python3 test/run-upstream-stdlib-sweep.py --modules-file test/upstream_std_modules_full.txt --strict",
    "test:stdlib-sweep:full:go-test": "python3 test/run-upstream-stdlib-sweep.py --modules-file test/upstream_std_modules_full.txt --strict --go-test",
    "test:perf:go": "bash scripts/ci/perf-go-profiles.sh",
    "test:perf:go:update-baseline": "bash scripts/ci/perf-go-profiles.sh --update-baseline",
    "examples:compile": "python3 test/run-examples.py --compile-only",
    "examples:bless": "python3 test/run-examples.py --bless-generated",
    "examples:sync": "python3 scripts/examples/sync-generated.py",
    "examples:bins": "bash scripts/examples/build-binaries.sh",
    "hooks:install": "bash scripts/install-git-hooks.sh",
    "security:gitleaks": "bash scripts/security/run-gitleaks.sh",
    "security:deps": "bash scripts/security/run-dependency-audit.sh",
    "release:status": "bash scripts/release/check-release-state.sh",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "lix": "^15.0.0",
    "semantic-release": "^24.2.2"
  }
}
